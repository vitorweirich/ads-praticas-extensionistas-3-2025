{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false
  },
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "vue/no-unused-components": "warn",
    "vue/multi-word-component-names": "off"
  }
}
